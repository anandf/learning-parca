{
    "spec": {
        "template": {
            "containers": [{
                    "name": "parca",
                    "volumeMounts": [{
                        "mountPath": "/var/lib/parca",
                        "name": "data"
                    }]
                }],
            "spec": {
                "volumes": [{
                    "name": "data",
                    "persistentVolumeClaim": {
                        "claimName": "parca-data"
                    }
                }]
            }
        }
    }
}
